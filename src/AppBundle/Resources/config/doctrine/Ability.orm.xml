<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ability" table="Ability">
    <id name="idability" type="integer" column="idAbility">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lvl" type="integer" column="lvl" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isavtive" type="boolean" column="isAvtive" nullable="false"/>
    <field name="lvlreq" type="integer" column="lvlReq" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="img" type="string" column="img" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descrip" type="text" column="descrip" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="intreq" type="integer" column="intReq" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="lvlmax" type="integer" column="lvlMax" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
